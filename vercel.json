{
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@18.0.0"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/send-email", "dest": "/api/send-email.js" },
    {
      "src": "/professionals/detail",
      "status": 301,
      "headers": { "Location": "/professionals" }
    },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}