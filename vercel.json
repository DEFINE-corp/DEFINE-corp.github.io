{
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/send-email", "dest": "/api/send-email.js" },
    {
      "src": "/professionals/detail",
      "status": 301,
      "headers": { "Location": "/professionals" }
    },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}