{
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/api/:path*",
      "destination": "/pages/api/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/professionals/detail",
      "destination": "/professionals",
      "permanent": true
    }
  ],
  "builds": [
    {
      "src": "pages/api/send-email.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/send-email",
      "dest": "/pages/api/send-email.js"
    }
  ]
}